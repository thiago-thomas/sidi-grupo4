<div class="logo logo_dash">
    <a href="/admin"><img src="/imgs/logo.jpeg" alt="Logo Rei do Cangaço"></a>
    <div class="nome_novo">
        <span>Vendas</span>
        <a href="/admin/vendas/add"><button class="botao botao_nome_novo">Nova Venda</button></a>
    </div>
</div>
<div class="dashboard">
    {{#each order}}
    <div class="campo_obj">
        <div>
            <h3>Nome do Vendedor: {{usuario}}</h3>
            <h3>Quantidade de Produtos: {{cart.qtdTotal}}</h3>
            <h3>Valor Total: {{cart.valorTotal}}</h3>
        </div>
        <div>
        
        </div>
    </div>
    {{else}}
    <h4>Nenhuma Venda Registrada</h4>
    {{/each}}
</div>

